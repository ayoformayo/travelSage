<h1><%= @user.username %></h1>
<%= @user.name %>
<%= @user.handle %>
<%= @user.id %>
<% if @profile %>
  <%= link_to "Sign in with Facebook", "/auth/facebook", class: "sign_in" %>
  <%= link_to "Sign in with Twitter", "/auth/twitter", class: "sign_in" %>
<% end %>
<% @articles.each do |article| %>
  <%= link_to article.title, article_path(article) %><br>
<% end %>
